citHeader("To cite scSVA in publications use:")

citEntry(entry = "Article",
  title        = "scSVA",
  author       = personList(as.person("Marcin Tabaka"),
                   as.person("Aviv Regev")),
  journal      = "XX",
  year         = "2018",
  volume       = "1",
  number       = "1",
  pages        = "1--1",
  url          = "http://",

  textVersion  =
  paste("Marcin Tabaka, Aviv Regev (2018).",
        "scSVA:",
        "Journal,1(1), 1-21.",
        "URL http://")
)
